<!--The cad model refers to the HTCC shifted upstream by 1.94 cm which is the HTCC shift on Fall 2018 and after
    No additional shift needed here-->

<gxml>
	<volume name="htccMollerConeExt" color="888888" material="rohacell31" position="0*cm 0*cm 0*cm" rotation="0*deg 180*deg 0*deg" />
	<volume name="htccMollerCone"    color="888888" material="rohacell31" position="0*cm 0*cm 0*cm" rotation="0*deg 180*deg 0*deg" />
	<volume name="htccCone"          color="888888" material="rohacell31" position="0*cm 0*cm 0*cm" rotation="0*deg 180*deg 0*deg" />
</gxml>
